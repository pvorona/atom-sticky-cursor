{
  "name": "atom-sticky-cursor",
  "main": "./lib/index",
  "version": "0.2.0",
  "description": "Sticks cursor to beginning/end of line when moving up/down",
  "keywords": [
    "navigation",
    "productivity"
  ],
  "activationCommands": {
    "atom-text-editor": [
      "sticky-cursor:move-up",
      "sticky-cursor:move-up-by-3",
      "sticky-cursor:move-down",
      "sticky-cursor:move-down-by-3",
      "sticky-cursor:move-left",
      "sticky-cursor:move-right"
    ]
  },
  "repository": "https://github.com/pvorona/atom-sticky-cursor",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  }
}
